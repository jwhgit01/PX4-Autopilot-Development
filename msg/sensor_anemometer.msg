uint64 timestamp      # time since system start (microseconds)

uint32 device_id      # unique device ID for the sensor that does not change between power cycles

float32 u             # air-relative velocity in body frame x direction (m/s)
float32 v             # air-relative velocity in body frame y direction (m/s)
float32 w             # air-relative velocity in body frame z direction (m/s)

